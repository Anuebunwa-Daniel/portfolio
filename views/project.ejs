<%- include('./layouts/header') %>

      <h1 class="text-center"> Projects </h1>

        <div class="background">
          <div  id="po">
            <% posts.forEach(post=>{%>
              <div class="text-center"id="post">
                <div class="noBold">
                  <a data-fancybox="gallery" href="<%= post.url %>">
                  <img class="img-fluid" id="porject-img" src="<%= post.url %>" alt="">
